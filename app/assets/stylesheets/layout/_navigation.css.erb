nav.nav-extended{
  color: #1db5ab;
  height: 60px;
  position: relative;
  background-color: transparent;
  z-index: 999;
  border: none;
  box-shadow: none;
}

.nav-wrapper{
  padding: 20px;
}

.brand-logo{
  color: #fff !important;
}

.brand-logo img{
  width: 70px;
}

nav ul a{
  font-size: 1.2rem !important;
  text-transform: uppercase;
}

nav ul a:first{
  border: 1px solid #fff;
}

.header-text{
  text-align: center;
}

.text-content{
  font-size: 3rem;
  margin: 2rem 0 5rem 0;
  font-weight: bold;
}

.header-image{
  background-position: bottom;
  transition: all 0.25s ease;
  background-color: #4D4D4D;
  background-position: center;
  background-size: cover;
  color: #fff;
  margin-top: -64px;
  min-height: 480px;
  object-fit: cover;
  padding: 135px 0 66px;
  position: relative;
  z-index: 100;
}
